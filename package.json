    "dist": "webpack --config webpack.config-dev.js",
    "dist:prod": "webpack --config webpack.config-prod.js"
  },
  "repository": "git+https://github.com/kinotto/geonames.js.git",
  "author": "Karim Abdelcadir <kinotto88@yahoo.it>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kinotto/geonames.js/issues"
  },
  "dependencies": {
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^3.5.0",
    "coveralls": "^2.12.0",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "opener": "^1.4.1",
    "require-dir": "^0.3.1",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "tiny-lr": "^0.2.1",
